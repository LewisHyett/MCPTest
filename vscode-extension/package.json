{
  "name": "tes-al-quickfix",
  "displayName": "TES AL Quick Fix",
  "description": "Quick Fix actions for TES AL standards using the MCP propose_fixes engine.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onLanguage:al",
    "onCommand:tesAl.applyFileFixes",
    "onCommand:tesAl.proposeFileFixes"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tesAl.proposeFileFixes",
        "title": "TES: Propose fixes for current AL file"
      },
      {
        "command": "tesAl.applyFileFixes",
        "title": "TES: Apply fixes for current AL file"
      }
    ],
    "codeActions": [
      {
        "languages": ["al"],
        "kind": "quickfix"
      }
    ]
  },
  "publisher": "tes-internal",
  "license": "MIT"
}
